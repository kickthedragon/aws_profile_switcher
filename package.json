{
	"name": "aws_profile_switcher",
	"productName": "AWS Profile Switcher",
	"version": "1.0.5",
	"license": "MIT",
	"author": {
		"name": "Eric Remigino",
		"email": "eric.remigino@gmail.com",
		"url": "https://ericremigino.com"
	},
	"scripts": {
		"dev": "electron-webpack dev",
		"compile": "electron-webpack",
		"rebuild": "./node_modules/.bin/electron-rebuild",
		"rebuild-deps": "electron-builder install-app-deps",
		"dist": "yarn compile && electron-builder",
		"pub": "yarn compile && electron-builder --publish always",
		"dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
	},
	"build": {
		"appId": "com.ericremigino.awsProfileSwitcher",
		"productName": "AWS Profile Switcher",
		"publish": [
			{
				"provider": "github",
				"owner": "kickthedragon",
				"repo": "aws_profile_switcher"
			}
		],
		"dmg": {
			"contents": [
				{
					"x": 110,
					"y": 150
				},
				{
					"x": 240,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				}
			]
		}
	},
	"devDependencies": {
		"babel-eslint": "^10.0.1",
		"electron": "^4.1.3",
		"electron-builder": "^20.39.0",
		"electron-webpack": "^2.6.2",
		"eslint": "^5.16.0",
		"webpack": "^4.29.6"
	},
	"dependencies": {
		"aws-sdk": "^2.432.0",
		"command-exists": "^1.2.8",
		"electron-log": "^3.0.4",
		"electron-updater": "^4.0.6",
		"electron-util": "^0.11.0",
		"fix-path": "^2.1.0",
		"fs-extra": "^7.0.1",
		"lodash": "^4.17.11",
		"source-map-support": "^0.5.11"
	}
}
